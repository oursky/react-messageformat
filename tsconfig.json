{
  "compilerOptions": {
    "target": "ES5",
    "module": "ES2015",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "importHelpers": true,
    "lib": ["ES5", "DOM", "ES2015.Promise"],
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "src/**/*",
    "typings/**/*"
  ]
}
